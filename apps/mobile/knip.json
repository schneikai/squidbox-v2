{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["app/**/*.{ts,tsx}"],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "types/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "scripts/**/*.{ts,tsx,js}",
    "*.{ts,tsx,js,json}"
  ],
  "ignore": [
    "node_modules/**",
    ".expo/**",
    "dist/**",
    "build/**",
    "coverage/**",
    "*.config.js",
    "*.config.ts",
    "expo-env.d.ts"
  ],
  "ignoreDependencies": [
    "@expo/vector-icons",
    "@react-navigation/bottom-tabs",
    "@react-navigation/elements",
    "@rneui/base",
    "react-dom",
    "react-native-gesture-handler"
  ],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": true
}
